stud@debian:~$ mkdir q1
stud@debian:~$ cd q1
stud@debian:~/q1$ cat>apple
hi
stud@debian:~/q1$ cat>bat
hell
stud@debian:~/q1$ cat>cat
heello
stud@debian:~/q1$ cat>dell
do
stud@debian:~/q1$ cat>hell
fire
stud@debian:~/q1$ cat>1fill
1222
stud@debian:~/q1$ cat>23mca
dasuui
stud@debian:~/q1$ cat>5z
rew76rt6
stud@debian:~/q1$ cat>cabin123
hsdgwqed
